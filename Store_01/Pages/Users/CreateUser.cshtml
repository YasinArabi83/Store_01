@page
@model Store_01.Pages.Users.CreateUserModel
@{
    ViewBag.Title = "Create User";
}
<!-- Start Hero Section -->
<partial name="../Shared/partial/Hero.cshtml" />
<!-- End Hero Section -->
<div class="container my-5">
    <div asp-validation-summary="All" class="text-danger">

    </div>

    <form method="post">
        <div class="form-floating mb-3">
            <input asp-for="Email" type="email" class="form-control" id="floatingInput inputEmail3" placeholder="name@example.com">
            <label asp-for="Email" for="floatingInput">Email address</label>
        </div>
        <div class="form-floating mb-3">
            <input asp-for="UserName" type="text" class="form-control" id="floatingInput" placeholder="User Name">
            <label asp-for ="UserName" for="floatingInput">User Name</label>
        </div>
        <div asp-for="UserName" class="form-floating">
            <input asp-for="Password" type="password" class="form-control" id="floatingPassword" placeholder="Password">
            <label asp-for="Password" for="floatingPassword">Password</label>
        </div>
      
        <button type="submit" class="btn btn-primary my-4">Sign in</button>
    </form>
</div>
